<?xml version="1.0" encoding="UTF-8"?>
<suite parallel="false" name="NopCommerce - User">

	<listeners>
		<listener class-name="commons.MethodListener" />
<!--  		<listener class-name="reportConfig.ReportNGListener" /> -->
		<listener class-name="reportConfig.ExtentReportListenerV2" />
		<listener
			class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<parameter name="url"
		value="https://blueimp.github.io/jQuery-File-Upload/" />
	<test name="RUN on FIREFOX">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.nopcommerce.user.Level_15_ExtentV2_Screenshot" />
		</classes>
	</test>

</suite> 
